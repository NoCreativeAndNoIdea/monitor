datasource db {
  provider = "postgresql"
  url      = env("POSTGRE_DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Workspace {
  id    Int     @id @default(autoincrement())
  appId String  @default(uuid())
  name  String
  cover String?

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
